{
  "name": "yplayer",
  "version": "1.0.0",
  "description": "Youtube audio player for windows",
  "main": "dist/electron.js",
  "icon": "icons/logo48.png",
  "scripts": {
    "start": "webpack-dev-server  --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "el": "electron .",
    "packager": "electron-packager . --ignore=^/src",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "youtube",
    "audio",
    "player",
    "windows"
  ],
  "author": "Haikel Fazzani",
  "license": "MIT",
  "dependencies": {    
    "ytdl-core": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "electron": "^8.0.1",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "electron-builder": "^22.4.0"
  },
  "build": {
    "asar": false,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "installerIcon": "icons/logo48.png",
      "uninstallerIcon": "icons/logo48.png",
      "license": "License.md"
    },
    "directories": {
      "buildResources": "dist",
      "output": "build"
    },
    "files": [
      "dsit/electron.js",
      "dist/*",
      "dist/*/**",
      "!**/node_modules/*"
    ]
  }
}
